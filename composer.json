{
    "name": "illchuk/zfc-user-funky-session",
    "description": "When the PHP session references a ZfcUser identity that doesn't actually exist, things can get a little funky. This module serves as the preventative solution.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf3",
        "zfcuser"
    ],
    "homepage": "https://github.com/dillchuk/ZfcUserFunkySession",
    "authors": [
        {
            "name": "Derek Illchuk",
            "email": "derek@illchuk.com",
            "homepage": "http://www.derekillchuk.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "zf-commons/zfc-user": "^1.2 || ^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "ZfcUserFunkySession\\": "src/"
        }
    }
}
